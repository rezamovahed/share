  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrDemonWolf/share/issues"
  },
  "homepage": "https://github.com/MrDemonWolf/share#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.6.2",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "express-ip": "^1.0.4",
